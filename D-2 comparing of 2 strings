#include <iostream>  
using namespace std;  
  
int main ()  
{  
    string str1;  
    string str2;  
      
    cout << " Enter the String 1: " << endl;  
    cin >> str1;  
    cout << " Enter the String 2: " << endl;  
    cin >> str2;  
      
    if ( str1 == str2)  
    {  
        cout << " String is equal." << endl;  
    }  
    else  
    {  
        cout << " String is not equal." << endl;  
    }  
    return 0;  
}  
